      "url": "https://github.com/Azure/azure-docs-powershell-samples",
      "branch": "master",
      "branch_mapping": 
      {
        "release-build-mysql": "release-build",
        "release-build-postgresql": "release-build",
        "release-build-stellar": "release-build" 
      }
    }
  ],
  "branch_target_mapping": {
    "live": [
